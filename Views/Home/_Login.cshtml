@model LoginUser
<form action="/users/login" method="post">
            <div class="form-group">
                <label for="LogEmail">Email</label>
                <input asp-for="LogEmail" class="form-control">
                <span asp-validation-for="LogEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="LoginPassword">Password</label>
                <input asp-for="LoginPassword" class="form-control">
                <span asp-validation-for="LoginPassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-info">
            </div>
            </form>